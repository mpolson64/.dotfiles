[user]
    name = Miles Olson
    email = mpolson64@gmail.com

[alias]
	c = commit -am
	up = pull
	p = push
	s = status
	co = checkout
	df = diff --color --color-words --abbrev

[core]
    editor = emacs
    excludesfile = ~/.gitignore
	attributesfile = ~/.gitattributes
    
[push]
    default = simple
    
[help]
	autocorrect = 1

[credential]
	helper = cache
